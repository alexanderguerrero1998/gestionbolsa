@model ModalHeader

<div class="color-line"></div>
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h5 class="modal-title"  style="line-height:1em; height: 1em; overflow:hidden; cursor:help">@Model.Heading</h5>
    <small class="font-bold">@Model.Description</small>
</div>

 